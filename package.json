{
  "name": "task-env",
  "version": "1.2.0",
  "description":
    "A framework for building reusable JS tasks",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack",
    "fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "pretty": "prettier --write **/*.css **/*.json **/*.md",
    "lint": "eslint --ignore-path .gitignore --ext=js .",
    "test": "jest test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm test"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": ["prettier --write", "git add"],
    "*.js": [
      "eslint --ignore-path .gitignore --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 60,
    "semi": false,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/invrs/task-env.git"
  },
  "keywords": ["task", "exec", "cli", "json", "store"],
  "author": "Winton Welsh <winton@cyclo.ps>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/invrs/task-env/issues"
  },
  "homepage": "https://github.com/invrs/task-env#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.15.0-rc.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.0.1",
    "prettier": "^1.10.2",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "camel-dot-prop-immutable": "^1.2.0",
    "commandland": "0.0.8",
    "graceful-fs": "^4.1.11",
    "inquirer": "^5.0.1",
    "mri": "^1.1.0",
    "proper-lockfile": "^3.0.1",
    "structured-json": "^2.2.0"
  }
}
